                        <th class="text-left">-</th>
                        <th class="text-left">-</th>
                        <th class="text-left">-</th>
                        <th class="text-left">-</th>
                        <th class="text-left">-</th>
                        <th class="text-left">-</th>
                    </tr>
                </thead>
                <tbody class="table-hover">
                </tbody>
            </table>
        </div>
    </div>
    <script src="adminPanel.js"></script>
</body>

</html>